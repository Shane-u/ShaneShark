{
  "mcpServers": {
    "goodreads-neo4j": {
      "command": "uvx",
      "args": [
        "--native-tls",
        "mcp-neo4j-cypher@0.3.1",
        "--transport",
        "stdio"
      ],
      "env": {
        "NEO4J_URI": "bolt://localhost:7687",
        "NEO4J_USERNAME": "neo4j",
        "NEO4J_PASSWORD": "12345678",
        "NEO4J_DATABASE": "neo4j"
      }
    }
  }
}

